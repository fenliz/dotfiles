{
	"$schema": "https://aka.ms/terminal-profiles-schema",
	"defaultProfile": "{07b52e3e-de2c-5db4-bd2d-ba144ed6c273}",
	"alwaysShowTabs": false,
	"launchMode": "focus",
	"initialCols": 120,
	"initialRows": 60,
	"copyOnSelect": false,
	"copyFormatting": "none",
	"profiles": {
		"defaults": {
			"fontFace": "FiraCode Nerd Font Mono",
			"fontSize": 10,
			"colorScheme": "TokyoNight",
			"snapOnInput": true,
			"padding": "16, 0, 0, 0",
			"cursorShape": "bar",
			"startingDirectory": "%USERPROFILE%"
		},
		"list": [
			{
				"guid": "{07b52e3e-de2c-5db4-bd2d-ba144ed6c273}",
				"name": "Ubuntu",
				"commandline": "wsl",
				"startingDirectory": "//wsl$/Ubuntu-20.04/home/charlie",
				"padding": "4, 4, 0, 0",
				"scrollbarState": "hidden"
			},
			{
				"guid": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
				"name": "Windows PowerShell",
				"commandline": "powershell.exe"
			},
			{
				"guid": "{0caa0dad-35be-5f56-a8ff-afceeeaa6101}",
				"name": "Command Prompt",
				"commandline": "cmd.exe"
			}
		]
	},
	"schemes": [
		{
			"name": "TokyoNight",
			"black": "#363b54",
			"red": "#f7768e",
			"green": "#41a6b5",
			"yellow": "#e0af68",
			"blue": "#7aa2f7",
			"purple": "#bb9af7",
			"cyan": "#7dcfff",
			"white": "#787c99",
			"brightBlack": "#363b54",
			"brightRed": "#f7768e",
			"brightGreen": "#41a6b5",
			"brightYellow": "#e0af68",
			"brightBlue": "#7aa2f7",
			"brightPurple": "#bb9af7",
			"brightCyan": "#7dcfff",
			"brightWhite": "#acb0d0",
			"background": "#16161e",
			"foreground": "#787c99"
		}
	],
	"keybindings": [
		{
			"command": {
				"action": "copy",
				"singleLine": false
			},
			"keys": "ctrl+shift+c"
		},
		{
			"command": "paste",
			"keys": "ctrl+shift+v"
		},
		{
			"command": "find",
			"keys": "ctrl+shift+f"
		},
		{
			"command": "closePane",
			"keys": "ctrl+shift+w"
		},
		{
			"command": "newTab",
			"keys": "ctrl+shift+t"
		},
		{
			"command": null,
			"keys": "ctrl+,"
		}
	],
	"disabledProfileSources": [
		"Windows.Terminal.Wsl",
		"Windows.Terminal.Azure",
		"Windows.Terminal.PowershellCore"
	]
}
